<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="CloseProject">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="Diff.ApplyLeftSide" />
  <action id="Diff.ApplyRightSide" />
  <action id="ECTranslation">
    <keyboard-shortcut first-keystroke="meta i" />
    <keyboard-shortcut first-keystroke="alt i" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="Move">
    <keyboard-shortcut first-keystroke="shift f6" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="f6" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="UpdateRunningApplication" />
  <action id="ViewSource">
    <keyboard-shortcut first-keystroke="f4" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl f10" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="ctrl alt f10" />
  </action>
</keymap>